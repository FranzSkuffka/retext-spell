    "unist-util-visit": "^1.0.0"
  },
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/retext-spell.git"
  },
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "devDependencies": {
    "dictionary-en-gb": "^1.0.1",
    "dictionary-en-us": "^1.0.1",
    "dictionary-nl": "^1.0.1",
    "eslint": "^1.0.0",
    "istanbul": "^0.4.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "remark": "^3.1.0",
    "remark-comment-config": "^2.0.0",
    "remark-github": "^2.0.0",
    "remark-lint": "^2.0.0",
    "remark-slug": "^3.0.0",
    "remark-validate-links": "^2.0.0",
    "retext": "^1.0.0",
    "tape": "^4.2.0",
    "vfile-reporter": "^1.4.0"
  },
  "scripts": {
    "build-md": "remark . --quiet  --frail",
